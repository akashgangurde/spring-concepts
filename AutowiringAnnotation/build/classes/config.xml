<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-4.0.xsd">

	<context:annotation-config />

	<bean id="emp1" class="com.ag.Employee">
		<property name="id" value="23"></property>
		<property name="name" value="Lebron"></property>
	</bean>

	<bean id="emp2" class="com.ag.Employee">
		<property name="id" value="3"></property>
		<property name="name" value="AD"></property>
	</bean>


	<bean id="company" class="com.ag.Company">
		<property name="id" value="1"></property>
		<property name="name" value="LAL"></property>
	</bean>


</beans>