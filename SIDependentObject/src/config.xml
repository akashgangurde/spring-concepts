<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-4.0.xsd">

		<bean id="address1" class="com.ag.Address">
			<property name="city" value="LA"></property>
			<property name="state" value="CAL"></property>
		</bean>
	
		<bean id="emp1" class="com.ag.Employee">
			<property name="id" value="23"></property>
			<property name="name" value="Lebron"></property>
			<property name="address" ref="address1"></property>
		</bean>


</beans>
        